<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="group_openg2p_disbursement_advice_user" model="res.groups">
            <field name="name">Banking Officer</field>
            <field name="category_id" ref="openg2p.module_category_openg2p"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>


        <record id="group_openg2p_disbursement_advice_manager" model="res.groups">
            <field name="name">Banking Manager</field>
            <field name="category_id" ref="openg2p.module_category_openg2p"/>
            <field name="implied_ids"
                   eval="[(4, ref('openg2p_disbursement_advice.group_openg2p_disbursement_advice_user')), (4, ref('base.group_partner_manager'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <record id="openg2p_disbursement.group_openg2p_disbursement_manager" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('group_openg2p_disbursement_advice_manager'))]"/>
        </record>


        <record id="openg2p_disbursement.group_openg2p_disbursement_user" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('group_openg2p_disbursement_advice_user'))]"/>
        </record>


        <record id="base.default_user" model="res.users">
            <field name="groups_id"
                   eval="[(4, ref('group_openg2p_disbursement_advice_manager'))]"/>
        </record>

        <record id="base.user_admin" model="res.users">
            <field name="groups_id"
                   eval="[(4, ref('group_openg2p_disbursement_advice_manager'))]"/>
        </record>


    </data>
</odoo>
